{
  "name": "@aureooms/js-rational",
  "description": "Rational numbers for JavaScript",
  "version": "3.0.1",
  "license": "AGPL-3.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "homepage": "https://github.com/aureooms/js-rational",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-rational"
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-rational/issues"
  },
  "keywords": [
    "algorithm",
    "arithmetic",
    "bigdec",
    "bigdecimal",
    "complexity",
    "exact",
    "fractions",
    "javascript",
    "js",
    "numbers",
    "rational"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "install-hooks": "husky install",
    "lint": "true",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "dependencies": {
    "@aureooms/js-itertools": "^5.0.2"
  },
  "devDependencies": {
    "@aureooms/js-integer": "10.0.1",
    "@aureooms/js-integer-big-endian": "11.0.1",
    "@aureooms/js-number": "3.1.0",
    "@aureooms/js-prime": "4.0.0",
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "12.0.1",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "bn.js": "5.2.0",
    "c8": "7.6.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "5.2.0",
    "int": "0.2.0",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.38.2"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "defaults",
            "maintained node versions"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "debug": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ]
      }
    }
  }
}
